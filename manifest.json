{
	"manifest_version" : 3,
	"name" : "Chrome Extensions Manager",
	"version" : "5.0.1",
	"description" : "Chrome의 확장 프로그램을 효율적으로 관리합니다.",

	"icons" : {
		"16" : "icons/icon16.png",
		"32" : "icons/icon32.png",
		"48" : "icons/icon48.png",
		"128" : "icons/icon128.png"
	},

	"content_scripts" : [
		{
			"matches" : ["<all_urls>"],
			"js" : ["index.js"]
		}
	]
}